<!-- addcontact.ejs -->
<!DOCTYPE html>
<html lang="en">
  <!-- Pass a JavaScript variable to a partial in EJS -->
  <%- include('partials/head.ejs', { firstName: "Add Contact" }) %>

  <body>
    <%- include('partials/navbar') %>

    <div class="content">
      <div class="create">
        <h2>Add a New Contact</h2>
        <form action="/addContact" method="POST">
          <label>First Name:</label
          ><input type="text" value="" id="firstName" name="firstName" required />

          <label>Last Name:</label
            ><input type="text" value="" id="lastName" name="lastName" required />

          <label>Email:</label
              ><input type="text" value="" id="email" name="email" required />

          <label>Phone:</label
                ><input type="text" value="" id="phone" name="phone" required />

          <label>Address:</label
                  ><input type="text" value="" id="address" name="address" required />
    

          
          <button type="submit">Add Contact</button>
        </form>
      </div>
    </div>

    
  </body>
</html>